#include <stdio.h>

int main() {
    int year;

	scanf("%d",&year);
    
    // Read the year from the user
    
    
    // Determine if the year is a leap year using nested ternary operators
    const char *result = (year%400==0)? "Leap year":
	                     (year%100==0)? "Not a leap year":
	                      (year%4==0)? "Leap year" :
	                                    "Not a leap year";
	
    
    // Print the result
    printf("%s",result);
    
    return 0;
}
